{
    "name": "gutter/runner",
    "type": "project",
    "description": "Gutter Runner.",
    "keywords": ["gutter"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "cartalyst/stripe-laravel": "^13.1",
        "darkaonline/l5-swagger": "8.1",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fileeye/mimemap": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/cashier": "^12.13",
        "laravel/framework": "^8.12",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "league/mime-type-detection": "^1.7",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-permission": "^4.0",
        "tymon/jwt-auth": "^1.0@dev",
        "valorin/pwned-validator": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-stubs": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret --force --ansi"
        ],
        "post-update-cmd": [
            "@php artisan spatie-stub:publish"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
